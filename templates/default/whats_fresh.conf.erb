server {
  server_name seagrant-staging.osuosl.org;
  location /static/ {
    root <%= node['whats_fresh']['application_dir'] %>;
  }
  location /media/ {
    root <%= node['whats_fresh']['application_dir'] %>;
  }
  location / {
    proxy_pass http://localhost:<%= node['whats_fresh']['gunicorn_port'] %>;
  }
}
